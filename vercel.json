{
    "version": 2,
    "functions": {
    "src/index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 60
    }
  },
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }