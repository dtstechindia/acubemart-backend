{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node",
        "functions": {
          "index.js": {
            "maxDuration": 60
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }